/*4) Elabore um programa que indique se um número digitado está compreendido entre 20 e 90, ou não.*/
using System;

class Program {
  public static void Main (string[] args) {

    //Declaração de variáveis
    float num;

    //Entrada de dados
    Console.WriteLine ("Digite um número: ");
    num = float.Parse(Console.ReadLine());

    //Procesamento de dados e saída da informação
    if(num > 20 && num < 90){
      Console.WriteLine("O número digitado ESTÁ compreendido entre 20 e 90.");      
    }

    else{
      Console.WriteLine("O número digitado NÃO está compreendido entre 20 e 90.");
    }
  }
}
